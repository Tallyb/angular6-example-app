{
  "name": "angular-example-app",
  "version": "6.0.23",
  "license": "MIT",
  "scripts": {
    "start": "ng serve --open",
    "http-server": "http-server dist/ -c-1",
    "build": "ng build",
    "build:library": "ng build ngx-example-library",
    "deploy": "ng build --prod && angular-cli-ghpages",
    "test": "ng test",
    "test:library": "ng test ngx-example-library",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "lint:library": "ng lint ngx-example-library",
    "ci": "npm run lint && npm run lint:library && npm run test && npm run test:library && npm run e2e",
    "sme": "ng build && source-map-explorer dist/main.bundle.js",
    "release:minor": "standard-version --release-as minor && git push --follow-tags origin master",
    "release:major": "standard-version --release-as major && git push --follow-tags origin master",
    "docker": "docker build -t angularexampleapp . && docker run -d -p 4200:80 angularexampleapp",
    "sslgen": "node ./scripts/sslgen",
    "sslgen:install": "sudo security add-trusted-cert -d -r trustRoot -k /Library/Keychains/System.keychain ./scripts/ssl/server.crt"
  },
  "private": true,
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.0.0"
  },
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/flex-layout": "^7.0.0-beta.24",
    "@angular/forms": "^7.2.15",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/router": "^7.2.15",
    "classlist.js": "^1.1.20150312",
    "core-js": "2.6.1",
    "mcfly-semantic-release": "^1.1.7",
    "rxjs": "^6.5.2",
    "rxjs-compat": "^6.5.2",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.11.4",
    "@angular-devkit/build-ng-packagr": "^0.11.4",
    "@angular/cdk": "^7.3.7",
    "@angular/cli": "^7.3.9",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/language-service": "^7.2.15",
    "@angular/material": "^7.3.7",
    "@angular/platform-server": "^7.2.15",
    "@angular/service-worker": "^7.2.15",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@types/jasmine": "^3.3.12",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^10.14.6",
    "angular-cli-ghpages": "^0.5.3",
    "codelyzer": "^4.5.0",
    "coveralls": "^3.0.3",
    "hammerjs": "^2.0.8",
    "html2canvas": "^1.0.0-rc.1",
    "http-server": "0.11.1",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-remap-istanbul": "^0.6.0",
    "karma-scss-preprocessor": "^4.0.0",
    "moment": "2.22.1",
    "ng-packagr": "^4.7.1",
    "node-sass": "^4.12.0",
    "protractor": "^5.4.1",
    "selfsigned": "^1.10.4",
    "source-map-explorer": "^1.8.0",
    "standard-version": "^4.4.0",
    "ts-helpers": "^1.1.2",
    "ts-node": "^7.0.1",
    "tsickle": "^0.34.3",
    "tslib": "^1.9.3",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "webpack-cli": "^3.3.2"
  }
}
